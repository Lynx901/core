{% if __DEBUG__ is defined %}
<script type="text/javascript" async>
    var __debug = {
        includes: [],
        traces: []
    };
    {% for include in __DEBUG__.includes %}
    __debug.includes.push('{{ include }}');
    {% endfor %}
    {% for index, trace in __DEBUG__.trace %}
    __debug.traces.push({
    {% for key, value in trace %}
        {% if loop.index > 1 %},{% endif %}{{key}}: '{{ value | raw }}';
    {% endfor %}
    })
    {% endfor %}
</script>
{% endif %}
