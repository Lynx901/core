{% extends 'base.html.twig' %}
{% block body_header %}
    <div class="page-header">
        <h1>{{ "Opps, esto no era lo que me esperaba..."|trans }}</h1>
        <h2>{{ get_config('platform_name') }}</h2>
    </div>
{% endblock body_header %}
{% block body_content %}
    <p><strong>{{ exception.getCode }}</strong> {{ exception.getMessage }}</p>
    {% if __DEBUG__ %}
    <pre>{{ trace }}</pre>
    {% endif %}
{% endblock body_content %}
{% block scripts %}
{% endblock scripts %}
