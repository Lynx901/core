{% extends 'base.html.twig' %}
{% block body_header %}
    <div class="page-header">
        <h1>{{ "Herramienta de Gesti√≥n de Administradores PSFS"|trans }}</h1>
        <h2>{{ get_config('platform_name') }}</h2>
    </div>
{% endblock body_header %}
{% block body_content %}
<div class="panel panel-default">
    <div class="panel-body">
    {% for admin, hash in admins %}
    <span class="label label-primary">{{ admin }}</span>
    {% else %}
    <p class="well">{{ "No hay administradores definidos" }}</p>
    {% endfor %}
    </div>
</div>
    {{ form(form) }}
{% endblock body_content %}
{% block scripts %}
    <script type="text/javascript" src="{{ asset('/src/public/js/admins.js') }}"></script>
{% endblock scripts %}